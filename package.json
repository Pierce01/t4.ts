{
  "name": "t4ts",
  "version": "0.0.1",
  "description": "A simple terminalfour web api wrapper.",
  "type": "module",
  "exports": {
    "require": "./cjs/index.js",
    "import": "./esm/index.js",
    "default": "./esm/index.js"
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "./esm/index.d.ts"
      ]
    }
  },
  "main": "./cjs/index.js",
  "dependencies": {
    "node-fetch": "^3.3.1"
  },
  "scripts": {
    "build": "npx tsc --module es2022 --outDir esm/ && npx tsc --module commonjs --outDir cjs/"
  }
}
